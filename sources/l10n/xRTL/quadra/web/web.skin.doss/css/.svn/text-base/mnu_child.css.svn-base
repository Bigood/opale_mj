.childs{
	-webkit-border-radius: 20px;
	-moz-border-radius: 20px;
	border-radius: 20px;
	border:5px solid #a7a9ac;
	width:80%;
	padding-top:15px;
	padding-bottom:15px;
	margin:auto
}

.childs_co{
