/* Outline Child CSS */
ul.sw_child_navList {
	margin: 0em;
	padding: 0em;
	list-style: none;
}

ul.sw_child_navList li a, ul.sw_child_navList li span{
	font-size: 1.3em;
	text-decoration: none;
	min-height: 35px;
	display: inline-block;
	padding-right: 30px;/* Opale RTL */
	padding-top:5px;
	cursor: hand;
	cursor: pointer;
	color:#5e5e5e;
}

ul.sw_child_navList li span{
	background-color: transparent
}

ul.sw_child_navList li a:hover span{
	color: #60749c;
}
ul.sw_child_navList .mnu_ueDiv {
	background: url(../img/deco/ueDivChild.png) no-repeat top right white;/* Opale RTL */
}
