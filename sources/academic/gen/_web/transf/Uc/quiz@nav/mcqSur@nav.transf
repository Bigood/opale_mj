<?xml version="1.0" encoding="UTF-8"?>
<sm:genericWebSiteTransf xmlns:sc="http://www.utc.fr/ics/scenari/v3/core" xmlns:sm="http://www.utc.fr/ics/scenari/v3/modeling">
	<sm:model sc:refUri="/academic/model/Uc/quiz/mcqSur.model"/>
	<sm:axis code="nav"/>
	<sm:navigation>
		<sm:page template="page">
			<sm:title>
				<sm:dynamicString>
					<sm:freeSelect xpath="if(op:exeM/sp:title,concat('￼Exercice : ￼',op:exeM/sp:title),'￼Exercice￼')"/>
				</sm:dynamicString>
			</sm:title>
			<sm:outlineClasses>
				<sm:fixedString value="mnu_trainUc"/>
			</sm:outlineClasses>
			<sm:mainZone format="xhtml">
				<scBlock class="mainContent" xmlns="http://www.w3.org/1999/xhtml">
					<sm:choose>
						<sm:when xpath="op:exeM/sp:title">
							<hx class="mainContent_ti">
								<sm:callModel view="fullTitleText">
									<sm:current/>
								</sm:callModel>
							</hx>
						</sm:when>
					</sm:choose>
					<sm:Widget sc:refUri="/academic/gen/_web/widg/util/noJsWarn.uiwidget"/>
					<sm:Widget sc:refUri="/academic/gen/_web/widg/refs/kwrdEntries.uiwidget"/>
					<div class="divQuiz">
						<sm:choose>
							<sm:when sc:refUri="/academic/gen/_web/cnd/solQuiz.cond">
								<sm:callModel view="xhtmlContent">
									<sm:current/>
								</sm:callModel>
							</sm:when>
							<sm:otherwise>
								<sm:callModel view="xhtmlContent">
									<sm:current axis="noSol"/>
								</sm:callModel>
							</sm:otherwise>
						</sm:choose>
					</div>
				</scBlock>
			</sm:mainZone>
			<sm:zone key="zIconPage" format="xhtml">
				<div class="decoTrainUc" xmlns="http://www.w3.org/1999/xhtml"/>
			</sm:zone>
			<sm:zone key="zLegal" format="xhtml">
				<sm:callModel view="xhtmlContent">
					<sm:current axis="legal"/>
				</sm:callModel>
			</sm:zone>
			<sm:zone key="zKeywords" format="text">
				<sm:callModel view="xhtmlContent">
					<sm:root axis="keywds"/>
				</sm:callModel>
				<sm:callModel view="xhtmlContent">
					<sm:current axis="keywds"/>
				</sm:callModel>
			</sm:zone>
		</sm:page>
	</sm:navigation>
</sm:genericWebSiteTransf>