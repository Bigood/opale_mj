<?xml version="1.0" encoding="UTF-8"?>
<sm:openUiWidget code="infoTooltip" xmlns:sc="http://www.utc.fr/ics/scenari/v3/core" xmlns:sm="http://www.utc.fr/ics/scenari/v3/modeling">
	<sm:content>
		<span class="hidden" xmlns="http://www.w3.org/1999/xhtml"> | </span>
		<sm:WTooltip widgetClass="tooltip" class="info" tagName="a" xmlns="http://www.w3.org/1999/xhtml">
			<sm:label>
				<span>￼;Informations￼</span>
			</sm:label>
			<sm:labelTitle>￼;Informations complémentaires...￼</sm:labelTitle>
			<sm:tooltip>
				<sm:behavior trigger="onclick" sticky="true"/>
				<sm:position>
					<sm:relativeToAnchor horizontal="rightOfElement" vertical="topAlign"/>
				</sm:position>
				<sm:dimensions maxWidth="300"/>
				<sm:basket key="footnotes" render="asTooltip" itemKey="info"/>
				<sm:closeButton caption="x" title="￼;Fermer￼"/>
				<sm:ttTitle>
					<sm:callZone key="ttTitle"/>
				</sm:ttTitle>
				<sm:ttContent>
					<sm:callZone key="ttContent"/>
				</sm:ttContent>
			</sm:tooltip>
		</sm:WTooltip>
	</sm:content>
</sm:openUiWidget>